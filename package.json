{
  "name": "webpack-init",
  "version": "1.0.0",
  "private": true,
  "description": "webpack init",
  "author": "Vasyl <v.tsyutsyk@gmail.com>",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config webpack.config.js --open",
    "dev-build": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "eslint": "eslint --ext .js,.vue src --color",
    "eslint-fix": "eslint --ext .js,.vue src --fix --color",
    "lint-and-build": "npm run dev-build && npm run eslint",
    "prod-build": "cross-env NODE_ENV=production webpack",
    "staging-build": "cross-env NODE_ENV=staging webpack",
    "stylelint": "stylelint \"**/*.{scss,vue,css}\" --syntax scss --color",
    "stylelint-fix": "stylelint \"**/*.{scss,vue,css}\" --syntax scss --fix --color"
  },
  "main": "main.js",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "normalize.css": "^8.0.1",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.39",
    "vue-3-meta": "^2.4.0",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.5",
    "@babel/plugin-proposal-json-strings": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.20.2",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/compiler-sfc": "^3.2.45",
    "autoprefixer": "^10.4.13",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.0",
    "browser-sync": "^2.27.10",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.0",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.5.1",
    "eslint-webpack-plugin": "^3.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.6",
    "img-loader": "^4.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.1",
    "postcss-html": "^1.5.0",
    "postcss-loader": "^7.0.1",
    "prerender-spa-wp5-plugin": "^3.5.1",
    "sass-loader": "^13.0.2",
    "stylelint": "^14.12.1",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "stylelint-webpack-plugin": "^3.3.0",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^4.1.1",
    "vue-loader": "^17.0.0",
    "vue-loader-plugin": "^1.3.0",
    "vue-server-renderer": "^2.7.10",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-hot-middleware": "^2.25.2",
    "webpack-merge": "^5.8.0"
  },
  "browserslist": "> 0.25%, not dead",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-and-build",
      "pre-push": "npm run lint-and-build"
    }
  },
  "keywords": [
    "js",
    "javascript",
    "webpack"
  ],
  "license": "ISC"
}
